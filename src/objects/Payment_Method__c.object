    <validationRules>
        <fullName>American_Express_Starts_With_3</fullName>
        <active>true</active>
        <errorConditionFormula>IF( ( ISPICKVAL( Card_Type__c , &apos;American Express&apos;)  &amp;&amp;   NOT(BEGINS( Card_Number__c , &apos;3&apos;) ) &amp;&amp; LEN(Card_Number__c)&gt;4) , true, false)</errorConditionFormula>
        <errorMessage>We are sorry but your card number is not of type &apos;American Express&apos;.Please enter appropriate card information.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Discover_starts_with_6</fullName>
        <active>true</active>
        <errorConditionFormula>IF( ( ISPICKVAL( Card_Type__c , &apos;Discover&apos;) &amp;&amp;  NOT(BEGINS( Card_Number__c , &apos;6&apos;) )  &amp;&amp; LEN(Card_Number__c)&gt;4 )   , true, false)</errorConditionFormula>
        <errorMessage>We are sorry but your card number is not of card type &apos;Discover&apos;.Please enter appropriate card information.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>MasterCard_starts_with_5</fullName>
        <active>true</active>
        <errorConditionFormula>IF( ( ISPICKVAL( Card_Type__c , &apos;MasterCard&apos;) &amp;&amp; NOT(BEGINS( Card_Number__c , &apos;5&apos;) )&amp;&amp; LEN(Card_Number__c)&gt;4) , true, false)</errorConditionFormula>
        <errorMessage>We are sorry but your card number is not of card type &apos;MasterCard&apos;.Please enter appropriate card information.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Require_4_digit_for_American_Express</fullName>
        <active>true</active>
        <errorConditionFormula>LEN(CVV_Code__c) &lt;&gt; 4 &amp;&amp;  TEXT(Card_Type__c) = &apos;American Express&apos;</errorConditionFormula>
        <errorMessage>Credit Cards of type American Express require a 4 digit CVV code.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Visa_Starts_with_4</fullName>
        <active>true</active>
        <errorConditionFormula>IF( ( ISPICKVAL( Card_Type__c , &apos;Visa&apos;) &amp;&amp; NOT(BEGINS( Card_Number__c , &apos;4&apos;)) &amp;&amp; LEN(Card_Number__c)&gt;4) , true, false)</errorConditionFormula>
        <errorMessage>We are sorry but your card number is not of card type &apos;Visa&apos;.Please enter appropriate card information.</errorMessage>
    </validationRules>

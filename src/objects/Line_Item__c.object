    <validationRules>
        <fullName>Event_Charge_Item_cannot_be_blank</fullName>
        <active>true</active>
        <errorConditionFormula>AND(NOT( ISBLANK( Reservation_line__r.Post_Close_Date_Time__c ) ), ISBLANK( Override_Date__c ))</errorConditionFormula>
        <errorMessage>This event has already been closed. Please enter in an override date for this event charge item.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>For_Adjustment_Qty_should_only_be_negat</fullName>
        <active>true</active>
        <errorConditionFormula>AND( RecordType.Name == &apos;Adjustment&apos; ,  Qty__c &gt; 0)</errorConditionFormula>
        <errorDisplayField>Qty__c</errorDisplayField>
        <errorMessage>Quantity should be negative for Adjustments.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>For_Charge_Qty_should_only_be_Positive</fullName>
        <active>true</active>
        <errorConditionFormula>AND( RecordType.Name == &apos;Charge&apos; ,  Qty__c &lt;= 0)</errorConditionFormula>
        <errorDisplayField>Qty__c</errorDisplayField>
        <errorMessage>Quantity should be positive for Charge items.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Service_and_tax_adjustment_Check</fullName>
        <active>true</active>
        <errorConditionFormula>RecordType.Name == &apos;Adjustment&apos; &amp;&amp;(ISNULL(Qty__c) || Qty__c == 0) &amp;&amp; (ISNULL(Service_Charge_Adjustment_Quantity__c) || Service_Charge_Adjustment_Quantity__c ==0) &amp;&amp; (ISNULL(Tax_Adjustment_Quantity__c) || Tax_Adjustment_Quantity__c==0)</errorConditionFormula>
        <errorMessage>You must enter Either Service charge adjustment quatity (OR) Tax adjustment quantity</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>negativeQty</fullName>
        <active>false</active>
        <errorConditionFormula>Qty__c  &lt;= 0</errorConditionFormula>
        <errorMessage>Quantity can not be negative or zero</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Tax_exempt_can_not_change_for_closed_evt</fullName>
        <active>true</active>
        <errorConditionFormula>AND(NOT( ISBLANK( Reservation_line__r.Post_Close_Date_Time__c ) ), CreatedDate &lt; Reservation_line__r.Post_Close_Date_Time__c, ISCHANGED( Tax_Exempt__c))</errorConditionFormula>
        <errorMessage>Cannot Check/ Un-Check the Tax Exempt Box for Line Items created before Event is Closed/Posted</errorMessage>
    </validationRules>
